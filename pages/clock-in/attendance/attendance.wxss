/* 考勤 */
.attendance-body {
  width: 100%;
  padding: 0 24rpx;
}

.attendance-add {
  width: 100%;
}

.attendance-add>image {
  width: 40rpx;
  height: 40rpx;
  margin: 24rpx 12rpx 0 0;
}
.attendance-add-text {
  width: 100%;
  padding: 24rpx 0;
  border-bottom: 1px solid #F2F2F2;
}
.attendance-add-text text:last-child {
  font-size: 24rpx;
  color: #A6A6A6;
  margin-top: 6rpx;
}

.attendance-list {
  padding-left: 54rpx;
}

.attendance-item {
  width: 100%;
  height: 90rpx;
  border-bottom: 1px solid #F2F2F2;
  color: #A6A6A6;
}