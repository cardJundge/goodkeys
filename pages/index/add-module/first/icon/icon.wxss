/* 图标选择 */

.icon-body {
}

.icon-title {
  width: 100%;
  padding: 24rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 12rpx;
}

.icon-title image {
  width: 100rpx;
  height: 100rpx;
}

.icon-content {
  padding: 0 27rpx;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  padding-bottom: 144rpx;
}

.icon-content>view {
  width: 116rpx;
  height: 116rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}

.icon-content>view .icon-item {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}

.is-active {
  width: 76rpx;
  height: 76rpx;
  border: 2rpx solid #14D680;
}

.icon-success {
  position: absolute;
  right: 18rpx;
  bottom: 18rpx;
}

.icon-success>view {
  border-radius: 50%;
  background: #fff
}

.btn-box {
  width: 100%;
  height: 120rpx;
  background: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}

.icon-btn {
  width: 702rpx;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #1a65ff;
  color: #fff;
  border-radius: 20rpx;
}


